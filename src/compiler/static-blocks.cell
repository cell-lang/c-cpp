  String* dependees_apply_code([LinkedDBVar, TargetHandlerId] handlers, [LinkedDBVar -> DBSymbol] vars) {
    code = ();
    for v <- isort(keys(vars)) {
      msg_var_name = dependee_msg_var_ref_name(v);
      args = (v, updater_var_ref_name(v));
      target = target_method_resol_expr(v, vars(v), msg_var_name, handlers);
      code = (code |
        "if (!is_blank(" & msg_var_name & "))",
        "  " & native_call(target, args)
      );
    }
    return code;


    String target_method_resol_expr(LinkedDBVar block_var, DBSymbol rel_auto_id, String msg_var_name, [LinkedDBVar, TargetHandlerId] handlers) {
      dependee_handlers = [h : h <- handlers(block_var, ?)];

      if |dependee_handlers| == 1 {
        handler = only(dependee_handlers);
        if handler != :unknown {
          msg_type = prg_data.handlers_msg_type(rel_auto_id)(handler);
          return native_apply_method_name(rel_auto_id, handler, msg_type);
        }
      }

      methods_array_name = msg_handler_apply_array(rel_auto_id);
      resolver_name = handler_dispatch_resolver_name(rel_auto_id);
      return methods_array_name & "[" & resolver_name & "(" & msg_var_name & ")]";
    }
  }


  String* dependees_reset_code([LinkedDBVar, TargetHandlerId] handlers, [LinkedDBVar -> DBSymbol] vars) {
    code = ();
    for v <- isort(keys(vars)) {
      msg_var_name = dependee_msg_var_ref_name(v);
      args = (updater_var_ref_name(v));
      target = target_method_resol_expr(v, vars(v), msg_var_name, handlers);
      code = (code |
        "if (!is_blank(" & msg_var_name & ")) {",
        "  " & native_call(target, args),
        "  " & msg_var_name & " = make_blank_obj();",
        "}"
      );
    }
    return code;


    String target_method_resol_expr(LinkedDBVar block_var, DBSymbol rel_auto_id, String msg_var_name, [LinkedDBVar, TargetHandlerId] handlers) {
      dependee_handlers = [h : h <- handlers(block_var, ?)];

      if |dependee_handlers| == 1 {
        handler = only(dependee_handlers);
        if handler != :unknown {
          msg_type = prg_data.handlers_msg_type(rel_auto_id)(handler);
          return native_reset_method_name(rel_auto_id, handler, msg_type);
        }
      }

      methods_array_name = msg_handler_reset_array(rel_auto_id);
      resolver_name = handler_dispatch_resolver_name(rel_auto_id);
      return methods_array_name & "[" & resolver_name & "(" & msg_var_name & ")]";
    }
  }


  String* updates_check_code(StaticBlock block, [TableVar] insertion_tables, [TableVar] deletion_tables) {
    mutated_tables = insertion_tables & deletion_tables;
    mutated_tables = sort_by_key(mutated_tables, arity(block.tables($)));

    insert_checked_foreign_keys = [k : k <- block.insert_checked_foreign_keys, insertion_tables(k.origin)];
    insert_checked_foreign_keys = sort_by_key(insert_checked_foreign_keys, order);

    delete_checked_foreign_keys = [k : k <- block.delete_checked_foreign_keys, insertion_tables(k.origin)];
    delete_checked_foreign_keys = sort_by_key(delete_checked_foreign_keys, order);

    check_code = ("" if mutated_tables != ());

    //## BAD BAD BAD: *_prepare(...) ENDS UP BEING CALLED EVEN WHEN ALL FOREIGN KEYS ARE STATICALLY CHECKED
    for t <- mutated_tables
      check_code = check_code & aux_prepare_code(t, block);

    insertion_tables_with_keys = isort([v : v <- insertion_tables, keys(block.tables(v)) != []]);
    if insertion_tables_with_keys != () {
      check_code = (check_code | "") if not ends_with_empty_line(check_code);
      for v <- insertion_tables_with_keys {
        table = block.tables(v);
        arity = arity(table);
        keys = keys(table);
        if arity == 2 and not is_symmetric(table) and keys == [[0], [1]] {
          fn_call = native_call_expr("double_key_bin_table_aux_check_keys", (v, pending_updates_var_ref_name(v), :mem_pool));
          check_code = (check_code | "if (!" & fn_call & ")", "  return false;");
        }
        else {
          for k <- keys {
            fn_call = check_call(v, table, k);
            check_code = (check_code | "if (!" & fn_call & ")", " return false;");
          }
        }
      }
    }

    check_code = (check_code | "") if insert_checked_foreign_keys != () and not ends_with_empty_line(check_code);

    for k <- insert_checked_foreign_keys {
      check_expr = foreign_key_forward_check_expr(k, block);
      check_code = (check_code | "if (!" & check_expr & ")", "  return false;");
    }

    check_code = (check_code | "") if delete_checked_foreign_keys != () and not ends_with_empty_line(check_code);

    for k <- delete_checked_foreign_keys {
      check_expr = foreign_key_backward_check_expr(k, block);
      check_code = (check_code | "if (!" & check_expr & ")", "  return false;");
    }

    check_code = () if skip_integrity_checks;
    check_code = (check_code | "", "return true;");

    return check_code;


    String* aux_prepare_code(TableVar var, StaticBlock block) {
      table_type = block.tables(var);
      needs_preparation = match (table_type)
        table()             = arity(table_type) != 1,
        mandatory_column()  = false,
        _                   = true;
      return () if not needs_preparation;
      method_name = native_table_method_prefix(table_type) & "_aux_prepare";
      args = (pending_updates_var_ref_name(var));
      return (native_call(method_name, args));
    }


    String check_call(TableVar var, Table table, [+Nat] key) {
      assert table.keys(key);

      arity = arity(table);
      method_name = if      table.symmetric                    then semisym_table_key_check_method_names(arity, key, !)
                    else if arity == 2 and table.keys == [[0]] then "single_key_bin_table_aux_check_key_1"
                                                               else table_key_check_method_names(arity, key, !);
      aux_var = pending_updates_var_ref_name(var);
      args = (var, aux_var, :mem_pool if arity == 2 or (arity == 3 and (key == [0, 1] or key == [2])));
      return native_call_expr(method_name, args);
    }

    String check_call(TableVar var, MasterBinTable table, [+Nat] key) = undefined;

    String check_call(TableVar var, SlaveTernTable table, [+Nat] key) {
      method_name = slave_tern_table_key_check_method_names(key, !);
      aux_var = pending_updates_var_ref_name(var);
      args = (table.master, var, pending_updates_var_ref_name(table.master) if key == [0, 1], aux_var);
      return native_call_expr(method_name, args);
    }

    String check_call(TableVar var, MandatoryColumn column, [+Nat] key) {
      method_name = native_table_method_prefix(column) & "_aux_check_key_1";
      aux_var = pending_updates_var_ref_name(var);
      args = (column.master, var, aux_var, :mem_pool);
      return native_call_expr(method_name, args);
    }

    String check_call(TableVar var, Column column, [+Nat] key) {
      method_name = native_column_method_prefix(column.repr) & "_aux_check_key_1";
      aux_var = pending_updates_var_ref_name(var);
      args = (var, aux_var, :mem_pool);
      return native_call_expr(method_name, args);
    }

    String check_call(TableVar var, SlaveColumn column, [+Nat] key) {
      method_name = native_column_method_prefix(column.repr) & "_aux_check_key_1";
      aux_var = pending_updates_var_ref_name(var);
      args = (var, aux_var, :mem_pool);
      return native_call_expr(method_name, args);
    }


    String foreign_key_forward_check_expr(BlockForeignKey key, StaticBlock block) {
      method_name = foreign_key_forward_checker_name(key.type, block.tables(key.origin), block.tables(key.target));
      args = (key.origin, pending_updates_var_ref_name(key.origin), key.target, pending_updates_var_ref_name(key.target));
      return native_call_expr(method_name, args);
    }


    String foreign_key_backward_check_expr(BlockForeignKey key, StaticBlock block) {
      method_name = foreign_key_backward_checker_name(key.type, block.tables(key.origin), block.tables(key.target));
      needs_origin_ref = match (block.tables(key.target))
        table() t?  = arity(t) != 1 ,
        _           = true;
      args = (key.target if needs_origin_ref, pending_updates_var_ref_name(key.target), key.origin, pending_updates_var_ref_name(key.origin));
      return native_call_expr(method_name, args);
    }


    [NzNat, [+Nat], String] table_key_check_method_names = [
      2,  [0],    "bin_table_aux_check_key_1";
      2,  [1],    "bin_table_aux_check_key_2";
      3,  [2],    "tern_table_aux_check_key_3";
      3,  [0, 1], "tern_table_aux_check_key_12";
      3,  [1, 2], "tern_table_aux_check_key_23";
      3,  [0, 2], "tern_table_aux_check_key_13"
    ];


    [NzNat, [+Nat], String] semisym_table_key_check_method_names = [
      3,  [2],    "semisym_tern_table_aux_check_key_3";
      3,  [0, 1], "semisym_tern_table_aux_check_key_12"
    ];


    [[+Nat] -> String] slave_tern_table_key_check_method_names = [
      [0, 1]  -> "slave_tern_table_aux_check_key_12",
      [2]     -> "slave_tern_table_aux_check_key_3"
    ];


    Nat order((TableVar, NzNat, [+Nat]) key) {
      _, arity, args = key;
      return key_check_orders(arity, args, !);
    }


    [NzNat, [+Nat], Nat] key_check_orders = [
      2,  [0],      0;
      3,  [0, 1],   0;
      2,  [1],      1;
      3,  [1, 2],   1;
      3,  [2],      2;
      3,  [0, 2],   2
    ];


    Nat order(BlockForeignKey key) =
      match (key.type)
        unary_unary             = 0,
        unary_binary(i?)        = i,
        unary_ternary(i?)       = i,

        binary_unary(i?)        = i,
        binary_ternary          = 0,

        ternary_unary(i?)       = i,
        ternary_binary          = 0,

        unary_sym_binary        = 0,
        sym_binary_unary        = 0,

        unary_sym_ternary_12    = 0,
        unary_sym_ternary_3     = 2,

        sym_ternary_unary_12    = 0,
        sym_ternary_unary_3     = 2,

        sym_binary_sym_ternary  = 0,
        sym_ternary_sym_binary  = 0;
  }
}


## THIS ONE PROBABLY BELONGS SOMEWHERE ELSE
ValueStoreId+ table_value_stores(AnyTable table) =
  table()             = if table.symmetric then nonempty(drop(table.value_stores, 1)) else table.value_stores,
  master_bin_table()  |
  slave_tern_table()  = {assert not table.symmetric; return table.value_stores;},
  mandatory_column()  |
  column()            = (table.value_store),
  slave_column()      = table.value_stores;
