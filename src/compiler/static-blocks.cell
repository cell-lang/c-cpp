  //   return (sgn) & indent_lines(body) & ("}");
  // }


  String* var_apply_code(Atom var_id) {
    psv_str = native_var_ref_name(pending_state_var(var_id));
    mv_str = native_var_ref_name(memb_var(var_id));
    pool_ref_str = "&" & auto_or_db_arg_name & "->mem_pool";
    return (
      "if (!is_blank(" & psv_str & "))",
      "  " & mv_str & " = copy_to_pool(" & pool_ref_str & ", " & psv_str & ");"
    );
  }


  String native_user_method_decl(DBSymbol block_name, MethodId method_id, OptCompMethod method) {
    args_str = append((native_var_type(a) & ", " : a <- method.args));
    return native_type(method.ret_val_repr) & " " & user_def_method_name(block_name, method_id) & "(" & target_arg_decl(block_name) & ", " & args_str & "ENV &env);";
  }

  String* native_user_def_method_code(DBSymbol block_name, MethodId method_id, OptCompMethod method) {
    args_str = append((native_var_type(a) & " " & native_var_decl_name(a) & ", " : a <- method.args));
    sgn = native_type(method.ret_val_repr) & " " & user_def_method_name(block_name, method_id) & "(" & target_arg_decl(block_name) & ", " & args_str & "ENV &env) {";
    vars_decls = gen_vars_decls(method.body, set(method.args));
    body_text = vars_decls & ("" if vars_decls != ()) & native_code(method.body, nothing, break_target = nothing);
    return (sgn) & indent_lines(body_text) & ("}");
  }


  // String* native_user_def_method_code(DBSymbol block_name, MethodId method_id, Maybe[StdVar]* args, Instr* body) {
  //   final_args = (if a != nothing then value(a) else fn_arg(i) : a @ i <- args);
  //   args_str = append(("OBJ " & native_var_decl_name(a) & ", " : a <- final_args));
  //   sgn = "OBJ " & user_def_method_name(block_name, method_id) & "(" & args_str & "ENV &env) {";
  //   vars_decls = gen_vars_decls(body, [value(a) : a <- args, a != nothing]);
  //   body_text = vars_decls & ("" if vars_decls != ()) & native_code(body, nothing, break_target = nothing);
  //   return (sgn) & indent_lines(body_text) & ("}");
  // }

  String *handler_methods_arrays(StaticBlock block) {
    num_msg_handlers = |block.handlers|;
    array_size_str = _print_(num_msg_handlers);
    rel_auto_type = native_name(block.name);
    rel_auto_aux_type = native_auxiliary_name(block.name);

    msg_types = (block.handlers(handler_id(i)).msg_type : i < num_msg_handlers);

    array_name = msg_handler_entry_point_array(block.name);
    defs = ("", "bool (*" & array_name & "[" & array_size_str & "])(" & rel_auto_type & " *, " & rel_auto_aux_type & " *, OBJ, ENV &) = {");
    for i < num_msg_handlers {
      line = msg_handler_entry_point_name(block.name, handler_id(i), msg_types(i));
      line = line & "," if i != num_msg_handlers - 1;
      defs = (defs | "  " & line);
    }
    defs = (defs | "};", "");

    array_name = msg_handler_array(block.name);
    defs = (defs | "", "bool (*" & array_name & "[" & array_size_str & "])(" & rel_auto_type & " *, " & rel_auto_aux_type & " *, OBJ, ENV &) = {");
    for i < num_msg_handlers {
      line = native_handler_name(block.name, handler_id(i), msg_types(i));
      line = line & "," if i != num_msg_handlers - 1;
      defs = (defs | "  " & line);
    }
    defs = (defs | "};");

    array_name = msg_handler_apply_array(block.name);
    defs = (defs | "", "void (*" & array_name & "[" & array_size_str & "])(" & rel_auto_type & " *, " & rel_auto_aux_type & " *) = {");
    for i < num_msg_handlers {
      line = native_apply_method_name(block.name, handler_id(i), msg_types(i));
      line = line & "," if i != num_msg_handlers - 1;
      defs = (defs | "  " & line);
    }
    defs = (defs | "};");

    array_name = msg_handler_reset_array(block.name);
    defs = (defs | "", "void (*" & array_name & "[" & array_size_str & "])(" & rel_auto_aux_type & " *) = {");
    for i < num_msg_handlers {
      line = native_reset_method_name(block.name, handler_id(i), msg_types(i));
      line = line & ", " if i != num_msg_handlers - 1;
      defs = (defs | "  " & line);
    }
    defs = (defs | "};");

    return defs;
  }

  String* handler_resolution_code(StaticBlock block) {
    if block.handler_dispatch_resolver != nothing {
      arg, body = value(block.handler_dispatch_resolver);
      sgn = "uint32 " & handler_dispatch_resolver_name(block.name) & "(OBJ " & native_var_decl_name(arg) & ") {";
      return (sgn) & indent_lines(native_code(body, nothing, break_target = nothing)) & ("}");
    }
    else
      return ();
  }


  String** native_handlers_code(StaticBlock b) {
    sorted_handlers = _isort_([k : k _ <- b.handlers]);
    let block = b
      code = (native_handler_code(b.name, id, b.handlers(id).msg_type, b.handlers(id).body) : id <- sorted_handlers);
    return code;
  }
}


implicit prg_data : PrgData, rel_vars_perm_map : [DBSymbol -> [RelVar -> [+Nat -> Nat]]], context : <core>, block : StaticBlock, debug_mode : Bool, skip_integrity_checks : Bool {
  String* native_handler_code(DBSymbol block_name, HandlerId handler_id, NeType msg_type, Instr* body) {
    arg = :var(:this);
    args_decls = (target_arg_decl(block_name), auxiliary_arg_decl(block_name), obj_type_name & " " & native_var_decl_name(arg), "ENV &env");
    args_str = ("OBJ " & native_var_decl_name(arg) & ", ");
    sgn = "bool " & native_handler_name(block_name, handler_id, msg_type) & "(" & append(args_decls, ", ") & ") {";
    vars_decls = gen_vars_decls(body, [arg]);
    body_text = vars_decls & ("" if vars_decls != ()) & native_code(body, nothing, break_target = nothing);
    body_text = body_text & run_and_check_dependees_and_check_code(handler_id);

    main_method = (sgn) & indent_lines(body_text) & ("}");
    apply_and_reset_methods = handler_apply_and_reset_methods(handler_id, msg_type);
    entry_point_method = handler_entry_point_method(handler_id, msg_type);

    return main_method & apply_and_reset_methods & entry_point_method;


    String* run_and_check_dependees_and_check_code(HandlerId handler_id) {
      dependees = [a -> linked_db_var_type(block, a) : a _ <- updated_dependees(handler_id, ?, ?)];
      dependees_code = dependees_run_and_check_code(block.name, dependees);

      insertion_tables = [t : t <- table_vars_with_insertions(handler_id, ?)];
      deletion_tables = [t : t <- table_vars_with_deletions(handler_id, ?)];
      check_code = updates_check_code(block, insertion_tables, deletion_tables);

      return dependees_code & check_code;
    }


    String* handler_apply_and_reset_methods(HandlerId handler_id, NeType msg_type) {
      memb_vars = [v : v <- updated_memb_vars(handler_id, ?)];
      value_stores = [v : v <- value_stores_with_insertions(handler_id, ?)];
      dependees_handlers = [d, t : d t <- updated_dependees(handler_id, ?, ?)];
      dependees = [a : a _ <- dependees_handlers];
      insertion_tables = [t : t <- table_vars_with_insertions(handler_id, ?)];
      deletion_tables = [t : t <- table_vars_with_deletions(handler_id, ?)];
      table_vars = insertion_tables & deletion_tables;
      tables = [t -> block.tables(t) : t <- table_vars];

      dependees_apply_code = dependees_apply_code(dependees_handlers, [a -> linked_db_var_type(block, a) : a <- dependees]);
      dependees_reset_code = dependees_reset_code(dependees_handlers, [a -> linked_db_var_type(block, a) : a <- dependees]);

      apply_code = updates_apply_code(dependees, memb_vars, value_stores, deletion_tables, insertion_tables);
      reset_code = updates_reset_code(memb_vars, value_stores, tables);

      args = (target_arg_decl(block.name), auxiliary_arg_decl(block.name));
      apply_sgn = "void " & native_apply_method_name(block.name, handler_id, msg_type) & "(" & append(args, ", ") & ") {";
      body = dependees_apply_code & apply_code;
      apply_method_text = ("", apply_sgn) & indent_lines(body) & ("}");

      reset_sgn = "void " & native_reset_method_name(block.name, handler_id, msg_type) & "(" & auxiliary_arg_decl(block.name) & ") {";
      reset_method_text = ("", reset_sgn) & indent_lines(append_with_sep((dependees_reset_code, reset_code))) & ("}");

      return apply_method_text & reset_method_text;
    }


    String* handler_entry_point_method(HandlerId handler_id, NeType msg_type) {
      target_name = auto_or_db_arg_name;
      aux_name = db_auxiliary_arg_name;

      method_name = msg_handler_entry_point_name(block.name, handler_id, msg_type);
      formal_args = (target_arg_decl(block.name), auxiliary_arg_decl(block.name), obj_type_name & " msg", env_arg_decl);

      run_and_check_method_name = native_handler_name(block.name, handler_id, msg_type);
      apply_method_name = native_apply_method_name(block.name, handler_id, msg_type);
      reset_method_name = native_reset_method_name(block.name, handler_id, msg_type);

      return (
        "",
        "bool " & method_name & "(" & append(formal_args, ", ") & ") {",
        "  try {",
        "    bool ok = " & run_and_check_method_name & "(" & target_name & ", " & aux_name & ", msg, " & env_arg_name & ");",
        "    if (ok)",
        "      " & apply_method_name & "(" & target_name & ", " & aux_name & ");",
        "    " & reset_method_name & "(" & aux_name & ");",
        "    return ok;",
        "  }",
        "  catch (...) {",
        "    " & reset_method_name & "(" & aux_name & ");",
        "    return false;",
        "  }",
        "}"
      );

      // main_handler_args = (target_name, aux_name, "msg", "env");
      // apply_method_args = (target_name, aux_name, "code");
      // body = (
      //   // "int depth = get_call_stack_depth();",
      //   "  switch_mem_stacks();",
      //   "",
      //   "try {",
      //   "  //## SET FLAG SIGNALLING WE'RE INSIDE A TRANSACTION",
      //   "  int code = " & update_name(block_name, main_handler) & "(" & append(main_handler_args, ", ") & ");",
      //   "  //## RESET TRANSACTION FLAG",
      //   "  " & native_apply_and_reset_method_name(block_name, main_handler) & "(" & append(apply_method_args, ", ") & ");",
      //   "  //## CLEAR THE ERROR MESSAGE",
      //   "  unswitch_mem_stacks();",
      //   "  clear_unused_mem();",
      //   "  return true;",
      //   "}",
      //   "catch (...) {",
      //   "  //## RESET TRANSACTION FLAG",
      //   "  //## " & auxiliary_reset_proc_name(block_name) & "(" & aux_name & ");",
      //   "  //## CLEAR THE ERROR MESSAGE",
      //   "  unswitch_mem_stacks();",
      //   "  clear_unused_mem();",
      //   "  return false;",
      //   "}"
      // );
      // formal_args = (target_arg_decl(block_name), auxiliary_arg_decl(block_name), obj_type_name & " msg", env_arg_decl);
      // sgn = "bool " & name & "(" & append(formal_args, ", ") & ") {";
      // return (sgn) & indent_lines(body) & ("}");

      // name = update_entry_point_name(block_name);


    }
  }


  String* updates_reset_code([MembVar] memb_vars, [ValueStoreId] value_stores, [TableVar -> AnyTable] tables) {
    memb_vars_code = (
      native_var_ref_name(pending_state_var(bare_id(v))) & " = make_blank_obj();"
      : v <- isort(memb_vars)
    );

    value_stores_code = (
      native_call(native_value_store_method_prefix(block.value_stores(vs)) & "_aux_reset", (updater_var_ref_name(vs)))
      : vs <- isort(value_stores)
    );

    tables_code = ({
      table = tables(v);
      prefix = match (table)
        mandatory_column()  = native_column_method_prefix(table.repr),
        _                   = native_table_method_prefix(table);
      return native_call(prefix & "_aux_reset", (pending_updates_var_ref_name(v)));
    } : v <- isort(keys(tables)));

    ## BAD BAD BAD AND MAYBE ALSO BUGGY
    masters_to_partially_reset = isort([t.master : _ t <- tables, t :: <SlaveTernTable, SlaveColumn>, not tables(t.master, _)]);
    masters_partial_reset_code = (native_call("master_bin_table_aux_partial_reset", (pending_updates_var_ref_name(t))) : t <- masters_to_partially_reset);

    return append_with_sep((memb_vars_code, value_stores_code, tables_code, masters_partial_reset_code));
  }


  String* updates_apply_code([LinkedDBVar] dependees, [MembVar] memb_vars, [ValueStoreId] value_stores_with_insertions, [TableVar] tables_with_deletions, [TableVar] tables_with_insertions) {
    code = join((var_apply_code(bare_id(v)) : v <- isort(memb_vars)));

    if value_stores_with_insertions != [] {
      value_stores_code = (
        native_call(native_value_store_method_prefix(block.value_stores(s)) & "_aux_apply_insertions", (s, updater_var_ref_name(s), :mem_pool))
        : s <- isort(value_stores_with_insertions)
      );
      code = (code | "") if code != ();
      code = code & value_stores_code;
    }

    ## BUG BUG BUG: THIS LINE OF CODE CRASHES THE COMPILER
    ## ord_table_vars = sort_by_key(tables_with_deletions & tables_with_insertions, arity(block.tables($)));

    tables = block.tables;
    ord_table_vars = sort_by_key(tables_with_deletions & tables_with_insertions, arity(tables($)));
    for v <- ord_table_vars {
      table = tables(v);
      value_stores = table_value_stores(table);
      owned_value_stores = (maybe(vs, block.value_stores_owners(vs) == phys_col(original_rel_var(v), i)) : vs @ i <- value_stores);
      table_code = table_updates_apply_code(v, table, tables_with_deletions(v), tables_with_insertions(v), owned_value_stores);
      code = code & table_code;
    }

    return code;


    String* table_updates_apply_code(TableVar table_var, Table table, Bool has_deletions, Bool has_insertions, Maybe[ValueStoreId]+ owned_value_stores) {
      arity = arity(table);
      method_prefix = native_table_method_prefix(table) & "_aux";
      table_aux_name = pending_updates_var_ref_name(table_var);

      value_stores_args = value_stores_args(owned_value_stores);

      // Ternary tables have a single apply method
      if arity == 3 {
        args = (table_var, table_aux_name) & value_stores_args & (:mem_pool);
        method_name = method_prefix & "_apply";
        return (native_call(method_name, args));
      }

      code = ();

      if has_deletions {
        args = (table_var, table_aux_name) & value_stores_args & (:mem_pool);
        method_name = method_prefix & "_apply_deletions";
        code = (code | native_call(method_name, args));
      }

      if has_insertions {
        args = (table_var, table_aux_name, :mem_pool);
        method_name = method_prefix & "_apply_insertions";
        code = (code | native_call(method_name, args));
      }

      if arity == 1 {
        masters_raw_columns = prg_data.masters_raw_columns(block.name);

        if masters_raw_columns(table_var, _) {
          capacity_var = native_var_decl_name(table_var) & "_capacity";
          new_capacity_var = native_var_decl_name(table_var) & "_new_capacity";

          capacity_expr = native_call_expr(native_table_method_prefix(table) & "_capacity", (native_var_ref_name(table_var)));

          resize_instrs = (
            native_call(native_table_method_prefix(block.tables(c)) & "_resize", (c, capacity_var, new_capacity_var, :mem_pool))
            : c <- isort([c : c <- masters_raw_columns(table_var, ?)])
          );

          code = (
            "uint32 " & capacity_var & " = " & capacity_expr & ";",
          ) & code & (
            "uint32 " & new_capacity_var & " = " & capacity_expr & ";",
            "if (" & capacity_var & " != " & new_capacity_var & ") {"
          ) & indent_lines(resize_instrs) & (
            "}"
          );
        }
      }

      return code;
    }

    String* table_updates_apply_code(TableVar table_var, MasterBinTable table, Bool has_deletions, Bool has_insertions, Maybe[ValueStoreId]+ owned_value_stores) {
      method_prefix = native_table_method_prefix(table) & "_aux";
      table_aux_name = pending_updates_var_ref_name(table_var);

      code = ();

      if has_insertions {
        args = (table_var, table_aux_name);
        method_name = method_prefix & "_apply_surrs_acquisition";
        code = (code | native_call(method_name, args));
      }

      if has_deletions {
        args = (table_var, table_aux_name) & value_stores_args(owned_value_stores) & (:mem_pool);
        method_name = method_prefix & "_apply_deletions";
        code = (code | native_call(method_name, args));
      }

      if has_insertions {
        args = (table_var, table_aux_name, :mem_pool);
        method_name = method_prefix & "_apply_insertions";
        code = (code | native_call(method_name, args));
      }

      return code;
    }


    String* table_updates_apply_code(TableVar table_var, SlaveTernTable table, Bool has_deletions, Bool has_insertions, Maybe[ValueStoreId]+ owned_value_stores) {
      method_prefix = native_table_method_prefix(table) & "_aux";
      table_aux_name = pending_updates_var_ref_name(table_var);

      code = ();

      if has_deletions {
        args = (table_var, table_aux_name) & value_stores_args(nonempty(drop(owned_value_stores, 2))) & (:mem_pool);
        method_name = method_prefix & "_apply_deletions";
        code = (code | native_call(method_name, args));
      }

      if has_insertions {
        args = (table_var, table_aux_name, :mem_pool);
        method_name = method_prefix & "_apply_insertions";
        code = (code | native_call(method_name, args));
      }

      return code;
    }


    String* table_updates_apply_code(TableVar table_var, Column column, Bool has_deletions, Bool has_insertions, Maybe[ValueStoreId]+ owned_value_stores) {
      method_prefix = native_table_method_prefix(column) & "_aux";
      table_aux_name = pending_updates_var_ref_name(table_var);

      code = ();

      if has_insertions and column.repr == :object {
        method_name = "obj_col_aux_copy_values_into_mem_pool";
        args = (table_aux_name, :mem_pool);
        code = (code | native_call(method_name, args));
      }

      if has_deletions {
        remove_method_name = native_value_store_method_prefix(block.value_stores(column.value_store)) & "_remove_untyped";
        args = (table_var, table_aux_name) & value_stores_args(owned_value_stores) & (:mem_pool);
        method_name = method_prefix & "_apply_deletions";
        code = (code | native_call(method_name, args));
      }

      if has_insertions {
        args = (table_var, table_aux_name, :mem_pool);
        method_name = method_prefix & "_apply_updates_and_insertions";
        code = (code | native_call(method_name, args));
      }

      return code;
    }


    String* table_updates_apply_code(TableVar table_var, MandatoryColumn column, Bool has_deletions, Bool has_insertions, Maybe[ValueStoreId]+ owned_value_stores) {
      method_prefix = native_table_method_prefix(column) & "_aux";
      table_aux_name = pending_updates_var_ref_name(table_var);
      master_aux_name = pending_updates_var_ref_name(column.master);
      args = (column.master, master_aux_name, table_var, table_aux_name, :mem_pool);
      method_name = method_prefix & "_apply";
      return (native_call(method_name, args));
    }


    String* table_updates_apply_code(TableVar table_var, SlaveColumn column, Bool has_deletions, Bool has_insertions, Maybe[ValueStoreId]+ owned_value_stores) {
      assert owned_value_stores == (nothing, nothing);

      method_prefix = native_table_method_prefix(column) & "_aux";
      table_aux_name = pending_updates_var_ref_name(table_var);

      code = ();

      if has_deletions {
        args = (table_var, table_aux_name) & value_stores_args((nothing)) & (:mem_pool);
        method_name = method_prefix & "_apply_deletions";
        code = (code | native_call(method_name, args));
      }

      if has_insertions {
        args = (table_var, table_aux_name, :mem_pool);
        method_name = method_prefix & "_apply_updates_and_insertions";
        code = (code | native_call(method_name, args));
      }

      return code;
    }

    ////////////////////////////////////////////////////////////////////////////

    Encodable* value_stores_args(Maybe[ValueStoreId]+ owned_value_stores) {
      vs_args = ();
      for maybe_vs <- owned_value_stores {
        if maybe_vs != nothing {
          vs = value(maybe_vs);
          remove_method_name = native_value_store_method_prefix(block.value_stores(vs)) & "_remove_untyped";
          vs_args = (vs_args | remove_method_name, vs);
        }
        else
          vs_args = (vs_args | "NULL", "NULL");
      }
      return vs_args;
    }
  }
}


implicit prg_data : PrgData, rel_vars_perm_map : [DBSymbol -> [RelVar -> [+Nat -> Nat]]], context : <core>, debug_mode : Bool, skip_integrity_checks : Bool {
